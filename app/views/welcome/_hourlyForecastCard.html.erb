<div class="card-deck-wrapper">
  <div class="card-deck">
    <% 3.times do %>
        <div class="card text-center" style="background-color: #446AAA; border-color: #446AAA;">
          <div class="card-header">
            <div class="card-block">
              <h4 class="card-title">    
                DATE
              </h4>
              <h6 class="card-subtitle text-muted">summary</h6>
            </div>
          </div>
          <div class="card-block card-inverse">
            <p><canvas id="<%="icon#{@skyconCount}"%>" class="card-img-top"  width="128" height="128"></canvas></p>

            <h4 class="card-title">HI &nbsp; <small id="loTemp">Lo</small></h4>
          </div>
          <ul class="list-group list-group-flush">
            <li class="list-group-item"> feels like a high of apparentHi<br> with a low ofapparentlow</li>
          </ul>
          <div class="card-footer text-muted">
              <i class="fa fa-umbrella"></i> &nbsp; % Chance of Rain
          </div>
        </div>
    <% end %>
  </div>
</div>